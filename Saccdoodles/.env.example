# SACC Doodles Environment Variables
# Copy this to .env and fill in your values
# Then copy the values to js/config.js

# Supabase Database
# Get from: Supabase Dashboard > Project Settings > API
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key

# Bunny.net CDN Storage (for photo/video uploads)
# Get API key from: Bunny Dashboard > Storage > saccdoodles > FTP & API Access > Password
BUNNY_STORAGE_HOST=https://la.storage.bunnycdn.com
BUNNY_STORAGE_ZONE=saccdoodles
BUNNY_CDN_URL=https://saccdoodles.b-cdn.net
BUNNY_API_KEY=your_bunny_storage_password_here

# Bunny Stream (optional - for video encoding)
BUNNY_STREAM_LIBRARY_ID=
BUNNY_STREAM_API_KEY=
